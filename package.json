    "draft-js": "^0.9.1",
    "draft-js-counter-plugin": "^2.0.0-beta7",
    "draft-js-plugins-editor": "^2.0.0-beta7",
    "immutable": "^3.8.1",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-helmet": "^3.1.0",
    "react-icons": "^2.2.1",
    "react-redux": "^4.4.6",
    "react-router": "^4.0.0-alpha.5",
    "redux": "^3.6.0"
  }
