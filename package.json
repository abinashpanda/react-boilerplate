{
  "name": "React boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate code for react projects.",
  "main": "src/index.js",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "test": "jest --watch",
    "clean": "rimraf dist",
    "copy": "cp favicon.png dist/",
    "start": "webpack-dev-server --config config/webpack.config.dev.js --host 0.0.0.0",
    "build": "npm run clean && webpack --config config/webpack.config.prod.js && npm run copy"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "./test/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.js$": "./test/transformer.js"
    },
    "collectCoverage": "true"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.1.0",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "css-loader": "^0.25.0",
    "enzyme": "^2.6.0",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-flowtype": "^2.25.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-mocha": "^4.7.0",
    "eslint-plugin-react": "^6.6.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.34.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^3.0.0",
    "jest": "^17.0.3",
    "jsdom": "^9.8.3",
    "postcss-loader": "^0.13.0",
    "react-addons-test-utils": "^15.4.0",
    "react-test-renderer": "^15.4.0",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  },
